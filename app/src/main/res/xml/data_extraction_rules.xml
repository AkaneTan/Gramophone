<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude />
    </cloud-backup>
    <!-- Playlists etc are stored in MediaStore, not in our data.
         Only "last played" is currently in our application data.
         When we later implement playback history or favourites,
         we might want to revisit this. -->
    <device-transfer>
        <exclude />
    </device-transfer>
</data-extraction-rules>